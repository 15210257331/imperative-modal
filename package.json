{
  "name": "imperative-modal",
  "version": "1.0.1",
  "type": "module",
  "main": "lib/imperative-modal.cjs.js",
  "module": "lib/imperative-modal.es.js",
  "types": "lib/types/index.d.ts",
  "exports": {
    "./lib/index.css": "./lib/index.css",
    ".": {
      "import": "./lib/imperative-modal.es.js",
      "require": "./lib/imperative-modal.cjs.js"
    }
  },
  "files": [
    "lib",
    "package.json"
  ],
  "scripts": {
    "dev": "vite",
    "build:doc": "vue-tsc --build tsconfig.lib.json && vite build --config vite.config.ts && node ./build/copyDemo.js",
    "build:lib": "vite build --config vite.config.lib.ts && vue-tsc --build tsconfig.lib.json",
    "release": "release-it",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "deploy": "node deploy.cjs"
  },
  "keywords": [
    "vue3",
    "vue3-modal",
    "vue3命令式模态框"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/15210257331/imperative-modal"
  },
  "dependencies": {
    "@tweenjs/tween.js": "^18.6.4",
    "@vueuse/core": "^9.13.0",
    "axios": "^1.7.0",
    "copy-dir": "^1.3.0",
    "element-plus": "^2.3.1",
    "highlight.js": "^11.7.0",
    "sass": "^1.59.3",
    "seemly": "^0.3.6",
    "vue": "^3.2.0",
    "vue-router": "^4.1.6"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.1.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.5",
    "prettier": "^2.8.7",
    "release-it": "^15.9.0",
    "ssh2": "^1.11.0",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-md": "^0.22.5",
    "vue-tsc": "^1.2.0"
  }
}
